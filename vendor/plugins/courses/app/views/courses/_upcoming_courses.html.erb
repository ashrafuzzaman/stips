<%# content_for :body_content_left do %>
<table class="homepage">
  <% @courses.each do |course| %>
    <tr class="<%= cycle('odd', 'even') %>">
      <td style="width: 100px;"><%= course.start_date %></td>
      <td><%= link_to course.title, course_url(course) %></td>
    </tr>
  <% end %>
</table>
